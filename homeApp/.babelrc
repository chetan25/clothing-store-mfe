{
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": false
      }
    ],
    [
      "@babel/preset-react",
      {
        "modules": false
      }
    ],
    [
      "@babel/preset-typescript",
      {
        "modules": false
      }
    ]
  ],
  "plugins": [
    [
      "@babel/plugin-transform-runtime",
      {
        "regenerator": true
      }
    ],
    ["babel-plugin-styled-components", { "pure": true, "ssr": false,
      "displayName": true,
      "namespace": "shell"
    }]
  ]
}
